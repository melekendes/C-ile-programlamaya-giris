#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	
	int multi[10][10];
	int calc;
	int i, j;
	
	for(i=1; i<=10; i++){
		
		printf("%dx..\n", i);
		for(j=1; j<=10; j++){
			printf("%dx%d=", i, j);
			calc = i * j;
			printf("%d\t", calc);
			printf("\n");
		}
		printf("\n");
	}

	return 0;
}
